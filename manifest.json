{
    "name": "Tab Man",
    "version": "0.1",
    "description": "Export the URL of all opened tabs to text",
    "icons": {
        "16": "tabs_io.png",
        "48": "tabs_io_48.png",
        "128": "tabs_io_128.png"
    },
    "action": {
        "default_icon": "tabs_io.png",
        "default_title": "Tab Man",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "tabGroups"],
    "manifest_version": 3
}
